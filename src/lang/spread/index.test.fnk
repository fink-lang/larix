{describe, it, expect, to_match_snapshot} = import '@fink/jest'

{parse_expr, parse} = import '../../'


describe:: 'spread', fn:

  it:: 'parses single line: ...foobar', fn:
    expect::
      parse_expr:: `...foobar`
      to_match_snapshot


  it:: 'parses standalone', fn:
    expect::
      parse_expr:: `[a, ..., b]`
      to_match_snapshot


  it:: 'parses multiple', fn:
    expect::
      parse_expr:: `
        (
          ...a
          ...b
        )`
      to_match_snapshot

