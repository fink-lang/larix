{add_operator_like} = import '@fink/prattler/symbols'
{infix} = import '../generic/infix'


comp = fn op: infix:: op, 'comp'


add_comparison_operators = fn ctx:
  pipe ctx:
    add_operator_like:: comp:: '=='
    add_operator_like:: comp:: '!='
    add_operator_like:: comp:: '>='
    add_operator_like:: comp:: '<='
    add_operator_like:: comp:: '>'
    add_operator_like:: comp:: '<'
