{describe, it, expect, to_match_snapshot} = import '../../testing/jest'

{parse_expr} = import '../../'


describe:: 'comparison', fn:

  it:: 'parses single line: a == b', fn:
    expect::
      parse_expr:: `a == b`
      to_match_snapshot


  it:: 'parses single line: a != b', fn:
    expect::
      parse_expr:: `a != b`
      to_match_snapshot


  it:: 'parses single line: a <= b', fn:
    expect::
      parse_expr:: `a <= b`
      to_match_snapshot


  it:: 'parses single line: a >= b', fn:
    expect::
      parse_expr:: `a >= b`
      to_match_snapshot


  it:: 'parses single line: a > b', fn:
    expect::
      parse_expr:: `a > b`
      to_match_snapshot


  it:: 'parses single line: a < b', fn:
    expect::
      parse_expr:: `a < b`
      to_match_snapshot

