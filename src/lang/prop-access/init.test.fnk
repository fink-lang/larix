{describe, it, expect, to_match_snapshot} = import '@fink/jest/test.fnk'

{parse, parse_expr} = import '../../parser.fnk'


describe 'prop access', fn:

  it 'parses single line', fn:
    expect
      parse_expr 'foo.bar'
      to_match_snapshot


  it 'parses various idents', fn:
    expect
      parse_expr 'foo.π'
      to_match_snapshot

    expect
      parse_expr 'foo.$spam'
      to_match_snapshot

    expect
      parse_expr 'foo._spam'
      to_match_snapshot

    expect
      parse_expr 'foo.∑'
      to_match_snapshot


  it 'parses with taken keywords', fn:
    expect
      parse_expr 'foo.match'
      to_match_snapshot

    expect
      parse_expr 'foo.fn'
      to_match_snapshot


  it 'parses member with str expr', fn:
    expect
      parse_expr "foo.'bar spam'"
      to_match_snapshot


  it 'parses member with group expr', fn:
    expect
      parse_expr 'foo.(key)'
      to_match_snapshot


  it 'errors with invalid member', fn:
    expect
      parse 'foo.[bar]', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.+bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.-bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.*bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo./bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.?bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.:bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.{bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.}bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.[bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.]bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.)bar', 'test.fnk'
      to_match_snapshot

    expect
      parse 'foo.,bar', 'test.fnk'
      to_match_snapshot



