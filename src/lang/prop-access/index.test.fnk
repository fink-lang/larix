{describe, it, expect, to_match_snapshot} = import '../../testing/jest'

{parse_expr} = import '../../'


describe:: 'prop access', fn:

  it:: 'parses single line: foo.bar', fn:
    expect::
      parse_expr(`foo.bar`)
      to_match_snapshot


  it:: `parses member expr: foo.'bar spam'`, fn:
    expect::
      parse_expr(`foo.'bar spam'`)
      to_match_snapshot


  it:: 'parses member expr: foo.(Symbol.iterator)', fn:
    expect::
      parse_expr('foo.(Symbol.iterator)')
      to_match_snapshot

