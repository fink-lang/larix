{describe, it, eq, snapshot} = import '../../testing/jest'

{parse_expr} = import '../../'


describe:: 'prop access', fn:

  it:: 'parses single line: foo.bar', fn:
    parse_expr(`foo.bar`) eq snapshot


  it:: `parses member expr: foo.'bar spam'`, fn:
    parse_expr(`foo.'bar spam'`) eq snapshot


  it:: 'parses member expr: foo.(Symbol.iterator)', fn:
    parse_expr('foo.(Symbol.iterator)') eq snapshot

