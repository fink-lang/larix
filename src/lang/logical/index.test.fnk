{describe, it, expect, to_match_snapshot} = import '../../testing/jest'

{parse_expr} = import '../../'


describe:: 'logical', fn:

  it:: 'parses single line: a && b || c', fn:
    expect::
      parse_expr(`a && b || c`)
      to_match_snapshot


  it:: 'parses single line: a || b && c', fn:
    expect::
      parse_expr(`a || b && c`)
      to_match_snapshot


  it:: 'parses single line: !a', fn:
    expect::
      parse_expr(`!a`)
      to_match_snapshot

