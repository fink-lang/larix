{add_non_separating} = import '@fink/prattler/symbols'

{prefix} = import '../symbols'


js_op = fn op, type: prefix:: op, type


add_js_ops = fn ctx:
  pipe ctx:
    # TODO: 'new' 'prefix' ?
    add_non_separating:: js_op:: 'new', 'new'
    # TODO: 'throw' 'prefix' ?
    add_non_separating:: js_op:: 'throw', 'throw'
