{describe, it, expect, to_match_snapshot} = import '../../testing/jest'

{parse_expr} = import '../..'


describe:: 'js-compat operators', fn:

  it:: 'parses new', fn:
    expect::
      parse_expr:: 'new_foo = new Foobar(spam, ni)'
      to_match_snapshot

    expect::
      parse_expr:: 'new_foo = new Foobar:: spam, ni'
      to_match_snapshot


  it:: 'parses throw', fn:
    expect::
      parse_expr:: `throw_foo = foo || throw err('foobar')`
      to_match_snapshot

    expect::
      parse_expr:: `throw_foo = foo || throw err:: 'foobar'`
      to_match_snapshot
