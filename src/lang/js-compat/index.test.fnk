{describe, it, expect, to_match_snapshot} = import '@fink/jest'

{parse_expr, parse} = import '../..'


describe 'js-compat operators', fn:
  it 'parses throw', fn:
    expect
      parse_expr "throw_foo = foo or throw err 'foobar'"
      to_match_snapshot


describe 'try ...:', fn:

  it 'parses', fn:
    expect
      parse "
        [err, result] = try:
          throw error 'foobar'
      "
      to_match_snapshot
