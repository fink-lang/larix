{add_keyword} = import '@fink/prattler/symbols.fnk'

{prefix} = import '../symbols.fnk'
{add_named_block} = import '../block/init.fnk'


js_op = fn op, type: prefix op, type


# TODO: do we want exceptions or should this live in @fink/js-interop?
add_js_ops = fn ctx:
  pipe ctx:
    add_keyword js_op 'throw', 'js-compat'
    add_named_block 'try', 'js-compat'
