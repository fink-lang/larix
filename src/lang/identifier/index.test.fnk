{describe, it, expect, to_match_snapshot, to_throw} = import '@fink/jest'

{parse_expr, parse} = import '../../'


describe:: 'other symbols', fn:
  it:: 'parses a word', fn:
    expect::
      parse_expr:: `  foobar  `
      to_match_snapshot


  it:: 'parses a non-word', fn:
    expect::
      parse_expr:: `  Ï€  `
      to_match_snapshot


describe:: 'symbols as prefix operators', fn:
  it:: 'parses with left to right precendence', fn:
    expect::
      parse:: `
        foo bar, spam, shrub
        foo bar, spam shrub
        foo
          bar
          spam
            1 + 23
            234
          ni(na)
        foo bar,
          fn bar:
            spam
          ni
            na
      `
      to_match_snapshot
