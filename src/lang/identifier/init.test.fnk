{describe, it, expect, to_match_snapshot} = import '@fink/jest/test.fnk'

{parse_expr} = import '../../parser.fnk'


describe 'indentifiers', fn:
  it 'parses a word', fn:
    expect
      parse_expr '  foobar  '
      to_match_snapshot


  it 'parses a non-word', fn:
    expect
      parse_expr ' Ï€ '
      to_match_snapshot

  it 'parses a hyphenated', fn:
    expect
      parse_expr ' foo-bar '
      to_match_snapshot
