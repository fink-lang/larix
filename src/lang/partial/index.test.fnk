{decribe, it, expect, to_match_snapshot, to_throw} = import '../../testing/jest'

{parse_expr} = import '../../'


describe:: 'partial', fn:

  it:: 'parses call', fn:
    expect::
      parse_expr:: `foo:: ?`
      to_match_snapshot

    expect::
      parse_expr:: `foo.bar:: ?`
      to_match_snapshot


  it:: 'parses member', fn:
    expect::
      parse_expr:: `?.bar()`
      to_match_snapshot


  it:: 'parses binary', fn:
    expect::
      parse_expr:: `? > 1`
      to_match_snapshot
    expect::
      parse_expr:: `1 < ?`
      to_match_snapshot
    expect::
      parse_expr:: `1 + ? * 3`
      to_match_snapshot


  it:: 'parses str', fn:
    expect::
      parse_expr:: `'foo \${?} bar'`
      to_match_snapshot

